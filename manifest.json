{
  "manifest_version": 3,
  "name": "HUBAI",
  "version": "1.0",
  "description": "Tracks web visits and intent",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "scripting", "activeTab", "idle"],
  "icons": {
    "16": "HUBAI.png",
    "48": "HUBAI.png",
    "128": "HUBAI.png"
  },
  "action": {
    "default_title": "HUBAI",
    "default_icon": "HUBAI.png"
  }
}
